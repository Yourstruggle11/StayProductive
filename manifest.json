{
    "manifest_version": 3,
    "name": "Stay Productive",
    "description": "Stay focused by blocking distracting websites for a certain amount of time",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}
